{
    "name": "nm-digitalhub/laravel-sumit-paymentfi",
    "description": "Laravel package for SUMIT Payment Gateway integration with Filament V4 Admin Panels",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "laravel",
        "payment",
        "gateway",
        "sumit",
        "filament",
        "creditcard",
        "israeli-clearing"
    ],
    "authors": [
        {
            "name": "NM Digital Hub",
            "email": "support@sumit.co.il"
        }
    ],
    "require": {
        "php": "^8.1|^8.2|^8.3",
        "illuminate/support": "^10.0|^11.0",
        "illuminate/database": "^10.0|^11.0",
        "illuminate/events": "^10.0|^11.0",
        "filament/filament": "^4.0",
        "guzzlehttp/guzzle": "^7.0",
        "spatie/laravel-settings": "^3.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0",
        "phpunit/phpunit": "^10.0|^11.0",
        "mockery/mockery": "^1.5"
    },
    "autoload": {
        "psr-4": {
            "NmDigitalHub\\SumitPayment\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NmDigitalHub\\SumitPayment\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "NmDigitalHub\\SumitPayment\\SumitPaymentServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
