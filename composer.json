{
    "name": "nm-digitalhub/woo-payment-gateway-officeguy",
    "description": "WooCommerce Payment Gateway with Laravel admin interface and package support",
    "type": "project",
    "license": "MIT",
    "keywords": [
        "laravel",
        "payment",
        "gateway",
        "sumit",
        "filament",
        "woocommerce",
        "creditcard",
        "israeli-clearing"
    ],
    "authors": [
        {
            "name": "NM Digital Hub",
            "email": "support@sumit.co.il"
        }
    ],
    "require": {
        "php": "^8.1|^8.2|^8.3",
        "laravel/framework": "^10.0|^11.0",
        "filament/filament": "^4.0",
        "guzzlehttp/guzzle": "^7.0",
        "spatie/laravel-settings": "^3.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0",
        "phpunit/phpunit": "^10.0|^11.0",
        "mockery/mockery": "^1.5",
        "nunomaduro/collision": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "NmDigitalHub\\SumitPayment\\": "src/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/",
            "NmDigitalHub\\SumitPayment\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "test": [
            "vendor/bin/phpunit"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "NmDigitalHub\\SumitPayment\\SumitPaymentServiceProvider"
            ]
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
