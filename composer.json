{
    "name": "nm-digitalhub/woo-payment-gateway-officeguy",
    "type": "library",
    "description": "WooCommerce Payment Gateway with modern Laravel admin interface powered by Filament v4 - Accept credit card payments via SUMIT/OfficeGuy clearing and invoicing system",
    "keywords": [
        "php",
        "laravel",
        "woocommerce",
        "payment-gateway",
        "filament",
        "sumit",
        "officeguy",
        "credit-card",
        "payments",
        "invoicing"
    ],
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "NM DigitalHub",
            "email": "info@nmdigitalhub.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "laravel/framework": "^11.0|^12.0",
        "filament/filament": "^4.1",
        "spatie/laravel-settings": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.0",
        "nunomaduro/collision": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "NmDigitalhub\\WooPaymentGatewayAdmin\\": "src/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage"
    },
    "extra": {
        "laravel": {
            "dont-discover": [],
            "providers": [
                "NmDigitalhub\\WooPaymentGatewayAdmin\\Providers\\PaymentPanelProvider"
            ]
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "version": "3.3.1"
}
